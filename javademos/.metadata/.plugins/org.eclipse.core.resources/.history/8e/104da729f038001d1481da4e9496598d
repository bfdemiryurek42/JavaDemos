package isVowelDemo;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		char character = 'a';
		char[] vowels = {'a','e','i','o','u'};

		
		for(char c: vowels ) {
			switch (c) {
			case 'a':
				System.out.println("");
			default:
				System.out.println();
			}
		}
		
	}

}
